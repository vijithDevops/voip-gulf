      <!-- /Left Sidebar Menu -->
      <!-- Right Sidebar Menu -->
      <style>
         .tab{
         background:#;
         height: 50px;
         }
      </style>
      <div class="page-wrapper" style="padding:0px 20px;">
         <div class="wrapper wrapper-content animated fadeInRight" style="padding: 1%;">
            <div class="row">
               <div class="ibox-content">
                  <br>
                  <div class="col-lg-10">
                     <center>
                        <br>
                        <h3>VCLOUD</h3>
                        <h5>Add Card</h5>
                        <br>
                        <br>
                        <br>
                     </center>
                  </div>
                  {% block content %}
                  <form class="form-horizontal" action="{% url "submitVcloudProducts" %}" method="post">
                    {% csrf_token %}
                     <div class="form-group">
                        <label class="col-lg-3 control-label">Brand:</label>
                        <div class="col-sm-4">
                           <select name="brand" class="form-control m-b">
                             {% for brand in brands %}
                             <option value="{{ brand.id }}">{{ brand.brand }}</option>
                             {% endfor %}
                           </select>
                        </div>
                        <div class="col-sm-2">
                           <a class="btn btn-sm btn-success" href="{% url "addvcloudbrand" %}">Add Brand</a>
                        </div>
                     </div>
                     <div class="form-group">
                        <label class="col-lg-3 control-label">Username:</label>
                        <div class="col-lg-5">
                          {{ productform.username }}
                        </div>
                     </div>
                     <div class="form-group">
                        <label class="col-lg-3 control-label">Password:</label>
                        <div class="col-lg-5">
                          {{ productform.password }}
                        </div>
                     </div>
                     <div class="form-group">
                        <div class="col-lg-10">
                           <center><button class="btn btn-sm btn-success" id="submit" type="submit">Submit</button></center>
                           <br>
                           <br><br>
                           <br>
                        </div>
                     </div>
                  </form>
                  {% if messages %}
                  {% for message in messages %}
                 <div {% if message.tags %} class="alert alert-{{ message.tags }}" {% endif %}>
                   <strong>{{ message.tag }}</strong> {{ message }}
                 </div>
                  {% endfor %}
                  {% endif %}
                  {% endblock %}
               </div>
            </div>
         </div>
